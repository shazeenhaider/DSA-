#print the first n natural  number
num=int(input("enter the value of num\n"))
i=0#initialization of iterator
while(i<num):
    print(i)
    i = i+1#updation of the iterator

print("loop ended succesfully")


#multiplication table in python programming
num=int(input("enter the number\n"))
i=1
while(i<=10):
    tab=num*i
    print(num,"X",i,"=",tab)
    i=i+1
print("loop completed ")    

#find the sum of first n natural number
num=int(input("enter the value of n\n"))
sum=0 #initialization
i=0
while(i<=num):
    sum=sum+i
    i=i+1
print("sum of the n natural number is",sum)

#factorial of a number in python
num=int(input("enter a number to find factorial\n"))
fact=1#initialize 
while(num>0):
    fact=fact*num #5! * 4! * 3! * 2! * 1! * 0!
    num=num-1
print("factorial of num is",fact)

#printing the grade according to the marks of the students
num=int(input("enter your score out of 100\n"))
if(num>=90):
    print("your grade is A")
elif(num>=80):
    print("your grade is B")
elif(num>=70):
    print("your grade is C")
elif(num>=60):
    print("your grade is D")
elif(num>=50):
    print("your grade id E")
else:
    print("FAIL")

#integer division in python
num=int(input("Enter the number of days\n"))
year=num//365#number of year
a=num%365
week=a//7#no of week
b=a%7#no of the days left
print(f" years:{year} week:{week} days:{b}" )

#check year is leap year or not
year=int(input("Enter a year\n"))
if(year%400==0) or (year%4==0 and year%100!=0):
    print(year,"year is a leap year")
else:
    print(year,"year is not a leap year")
#printing the fibonacci series
num=int(input("Enter the number of term you want\n"))
print(f"First {num} term of fibonacci series are: ")
first=0
second=1
if num >= 1:
    print(first, end=" ")
if num >= 2:
    print(second, end=" ")
for i in range(num-2):
    third=first+second
    print(third, end=" ")
    first=second
    second=third
#check whether a number is prime or not
num=int(input("Enter a number\n"))
flag=0
for i in range(2,num,1):
    if(num%i==0):
        print("number is not prime")
        break
else:
     print("number is prime")
    
#find the sum of all element of list
list1=[2,4,1,5,6,9]
sum=0
for i in range(0,len(list1),1):
    sum=sum+list1[i]
print(f"sum of all element of list1 {sum}")
#using slicing limited bounded sum of a list
list2=[3,2,4,1,6]
sum=0
for i in range(-2,-5,-1):
    sum=sum+list2[i]
print(f"sum of sliced list2 {sum}")
#create a user defined list for integer
list3=[]
siz=int(input("enter the size of list\n"))
for i in range(0,siz,1):
    b=int(input(f"Enter the values at {i}:\n"))
    list3.append(b)
print(f"printing the user defined list3 {list3}")

#sort a list of names according to thier length
list4=[]
siz=int(input("enter the size of list\n"))
#print(f"enter the names in list of size{siz}")
for i in range(0,siz,1):
    b=input(f"enter the name in the list at {i}\n")
    list4.append(b)
list4.sort(key=len,reverse=False)
print(list4)

#wap a program in python to count the number of vowel and constant
string=input("Enter a string check vowel/constant\n")
vow=0
cons=0
for ch in string:
    if ch.lower() in ['a','e','i','o','u']:#convert upper casetolower
        vow=vow+1
    elif ch.isalpha():#check for only english alphabets
        cons=cons+1
print(f"Number fo vowel {vow}")  
print(f"Number of constant {cons}")    

#Wap to find minimum and maximum in tuple
tup1=(2,3,4,1,6,7,8)
maxx=max(tup1)
minn=min(tup1)
print(f"Max and Min of the tuple {maxx} and {minn}")

#user defined tuple
ch=input("Enter element for tuple\n")
tup=ch.split()#default split by space
print(tup)
print("converting list to immutable tuple")
print(tuple(tup))

#user defined tuple without split func
size=int(input("Enter the size tuple \n"))
element=[]
for i in range(size):
    item=input(f"Enter item at {i+1}\n")
    element.append(item)
print(tuple(element))

#Wap to find repeated element in tuple
tup=(2,3,1,2,5,6,3)
list1=[]
for i in tup:
    if tup.count(i)>1 and i not in list1:
        list1.append(i)
con=tuple(list1)
print(con)
#Sort a tuple of tuples by the second element
tup=((2,3),(9,8),(5,4))
soreted=tuple(sorted(tup,key=lambda x:x[1]))
print(soreted)

#sum of 2 number using function
def cal_sum(a,b):
    print(a+b)
    return a+b
result=cal_sum(5,4)
print(f"sum of the number is {result}")

#find factorial using function in python
def cal_fact(a):
    if a==0 or a==1:
        return 1
    factorial=a*cal_fact(a-1)#recursion
    return factorial
num=int(input("enter the number to find factorial\n"))
result=cal_fact(num)
print(f"factorial of the number is {result}")

#wap using function to find fibonacci series
def cal_fibonacci(n):
    if  n<=1:
        return n
    else:
        return cal_fibonacci(n-1) + cal_fibonacci(n-2)

num = int(input("Enter the number of terms you want for Fibonacci series: "))
for i in range(num)Ä§
    print(cal_fibonacci(i), end=" ")

#fibonacci series using iteration for loop
def cal_fact(num):
    if num<=1:
        return num
    else:
        first=0
        second=1
        third=cal_fact(num-1)+cal_fact(num-2)
        first=second
        second=third
        return third
element=int(input("Enter the number of element of fibonacci series\n"))
for i in range(element):
    print(cal_fact(i),end=" ")







    
