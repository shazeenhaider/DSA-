#include <iostream>
#include <vector>
using namespace std;
int main(){
    vector<int> arr={1,2,3,5,6};//input 1--n
    int n=arr.size()+1;//finding the number of terms
    int total= n * (n+1)/2;//sum of n intergers using formula
    int sum=0;//initialising sum 
    for(int i=0; i<arr.size(); i++){
        sum += arr[i];//sum of all element of the vector till n 
    }
   cout<< total-sum;//subtracting the total(result) sum of vector element
return 0;
}
#include <iostream>
#include <vector>
using namespace std;
int main(){//time complexity O(n^2)
  vector<int> arr={1,4,3,5,6};
  int n = arr.size()+1;
  for(int count=1; count<=n; count++){
    bool found=false;
    for(int i=0; i<arr.size(); i++){
        if(arr[i]==count){
            found=true;
            break;
        }
    }
    if(!found){
        cout<<count;
    }
  }
  return 0;
}
